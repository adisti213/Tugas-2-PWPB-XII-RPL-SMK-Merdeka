# Tugas-2-PWPB-XII-RPL-SMK-Merdeka
<?php
$kategori = 'Buah Keras';
switch ($kategori)
{
  case 'Buah Keras' :
    echo $kategori . ' adalah ','apel, ','pir<br/>';
    break;
  case 'Buah halus' :
    echo $kategori . ' adalah ',' strawberry ',' anggur ',' blubery ',' ceri<br/>';
    break;
  case 'Buah tropis' :
    echo $kategori . ' adalah ',' nanas ',' jambu ',' mangga ',' rambutan<br/>';
    break;
}
?>
